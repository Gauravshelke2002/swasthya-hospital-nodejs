<%-include("navbar.ejs")%>

<form
  action="/admin/update_home_vision/<%=data._id%>"
  method="post"
  enctype="multipart/form-data"
>
  <div class="container p-4 bg-white">
    <div class="row">
      <div class="col-md-12">
        <h3>Edit home vision</h3>
      </div>
      <div class="col-md-12 mt-3">
        Image
        <input type="file" name="image" class="form-control" />
      </div>
      <div class="col-md-12 mt-3">
        
        <img src="/uploads/<%=data.image%>" width="100px" height="90px">
      </div>
      <div class="col-md-12 mt-3">
        Title
        <input type="text" name="title" value="<%=data.title%>" class="form-control" />
      </div>
      <div class="col-md-12 mt-3">
        Desc
        <textarea name="desc" class="form-control"><%=data.desc%></textarea>
      </div>
      <div class="col-md-12 mt-3">
        h1
        <input type="text" name="h1"value="<%=data.h1%>" class="form-control" />
      </div>
      <div class="col-md-12 mt-3">
        p1
        <input type="text" name="p1"value="<%=data.p1%>" class="form-control" />
      </div>
      <div class="col-md-12 mt-3">
        h2
        <input type="text" name="h2"value="<%=data.h2%>" class="form-control" />
      </div>
      <div class="col-md-12 mt-3">
        p2
        <input type="text" name="p2"value="<%=data.p2%>" class="form-control" />
      </div>
      <div class="col-md-12 mt-3 text-center">
        <button class="btn btn-primary">update home vision</button>
     </div>
    </div>
  </div>
</form>



<%-include("footer.ejs")%>