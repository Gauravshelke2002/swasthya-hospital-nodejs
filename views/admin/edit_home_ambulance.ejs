<%-include("navbar.ejs")%>

<form
  action="/admin/update_home_ambulance/<%=data._id%>"
  method="post"
  enctype="multipart/form-data"
>
  <div class="container p-4 bg-white">
    <div class="row">
      <div class="col-md-12">
        <h3>Edit Home ambulance</h3>
      </div>
      
      <div class="col-md-12 mt-3">
        H1
        <input type="text" name="h1"value="<%=data.h1%>" class="form-control" />
      </div>
      <div class="col-md-12 mt-3">
        H2
        <input type="text" name="h2"value="<%=data.h2%>" class="form-control" />
      </div>
      <div class="col-md-12 mt-3">
        H3
        <input type="text" name="h3" value="<%=data.h3%>" class="form-control" />

      </div>
      <div class="col-md-12 mt-3">
        Image
       <input type="file" name="image" class="form-control" />
     </div>
     <div class="col-md-12 mt-3">
       <img src="/uploads/<%=data.image%>" width="100px" height="90px">
     </div>
      

      <div class="col-md-12 mt-3 text-center">
        <button class="btn btn-primary">Update Home Dep part1</button>
     </div>
    </div>
  </div>
</form>



<%-include("footer.ejs")%>