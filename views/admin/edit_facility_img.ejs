<%-include("navbar.ejs")%>

<form
  action="/admin/update_facility_img/<%=data._id%>"
  method="post"
  enctype="multipart/form-data"
>
  <div class="container p-4 bg-white">
    <div class="row">
      <div class="col-md-12">
        <h3>Edit facility img</h3>
      </div>
      <div class="col-md-12 mt-3">
        Image
        <input type="file" name="image" class="form-control" />
      </div>
      <div class="col-md-12 mt-3">
       <img src="/uploads/<%=data.image%>" width="100px" height="90px">
      </div>
      <div class="col-md-12 mt-3 text-center">
        <button class="btn btn-primary">Update facility img </button>
     </div>
    </div>
  </div>
</form>



<%-include("footer.ejs")%>
